{
    "buildCommand": "cd frontend && npm install && npm run build",
    "outputDirectory": "frontend/dist",
    "builds": [
        {
            "src": "backend/server.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/reading",
            "dest": "backend/server.py"
        },
        {
            "src": "/api/season",
            "dest": "backend/server.py"
        },
        {
            "src": "/api/ranges",
            "dest": "backend/server.py"
        }
    ]
}